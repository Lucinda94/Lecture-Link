{
    "name": "lecture-link",
    "version": "1.0.0",
    "description": "Lecturer messaging app",
    "main": "server.js",
    "scripts": {
        "setup": "npm install && npm run initpgsql && npm install nodemailer",
        "initpgsql": "createdb lecturelink; psql lecturelink < init.sql",
        "loadDummyData": "psql lecturelink < dummydata.sql",
        "start": "node server.js",
        "devStart": "nodemon server.js",
        "test": "jest",
        "doc": "jsdoc -c jsdoc.json",
        "demo": "npm run setup && npm run loadDummyData && npm run start"
    },
    "dependencies": {
        "bcrypt": "^4.0.0",
        "body-parser": "^1.19.0",
        "ejs": "^2.5.5",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-session": "^1.17.0",
        "jest": "^25.1.0",
        "method-override": "^3.0.0",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "pg": "^7.18.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Lucinda94/Lecture-Link.git"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Lucinda94/Lecture-Link/issues"
    },
    "devDependencies": {
        "better-docs": "^1.4.7",
        "jsdoc": "^3.6.3",
        "jshint": "^2.11.0",
        "nodemon": "^2.0.2"
    }
}
